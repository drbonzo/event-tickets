{
	"defaultSeverity": "error",
	"extends": [
		"tslint:recommended"
	],
	"jsRules": {
		"no-unused-expression": true
	},
	"rules": {
		"member-access": [
			false
		],
		"ordered-imports": [
			false
		],
		"max-line-length": [
			false
		],
		"member-ordering": [
			false
		],
		"interface-name": [
			true,
			"never-prefix"
		],
		"arrow-parens": false,
		"no-empty-interface": false,
		"object-literal-shorthand": false,
		"object-literal-sort-keys": false,
		"object-literal-key-quotes": [
			true,
			"as-needed"
		],
		"no-namespace": [
			true,
			"allow-declarations"
		]
	},
	"rulesDirectory": []
}
